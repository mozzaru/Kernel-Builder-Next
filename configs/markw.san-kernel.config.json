    {
        "kernelSource": { 
            "name": "San-kernel", 
            "repo": "https://github.com/mozzaru/android_kernel_xiaomi_markw_new", 
            "branch": "lineage-22.1", 
            "device": "markw", 
            "defconfig": "markw_defconfig" 
          }, 
          "toolchains": [ 
